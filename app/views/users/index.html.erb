<h1>ユーザー一覧</h1>
<table>
  <tr>
    <th>ユーザーID</th>
    <th>ユーザー名</th>
    <th>メールアドレス</th>
    <th>詳細</th>
    <th>編集</th>
    <th>削除</th>
  </tr>
<% @users.each do |user| %>
  <tr>
    <td><%= user.id %></td>
    <td><%= user.name %></td>
    <td><%= user.email %></td>
    <td><%= link_to '詳細', user_path(user.id) %></td>
    <td><%= link_to '編集', edit_user_path(user.id) %></td>
  </tr>
<% end %>
</table>

<%= link_to 'ユーザー作成', '/users/new' %>