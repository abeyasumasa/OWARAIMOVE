<div class="container mt-5">
  <div class="wrapper">
    <div class="form-signin">
      <%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
        <%- flash.each do |name, msg| -%>
          <%= content_tag :div, msg, :id => "flash_#{name}" if msg.is_a?(String) %>
        <%- end -%>
        <h3 class="form-signin-heading">ログイン</h3>
        <hr class="colorgraph">
        <br>

<!--        <div class="for?m-control">-->
          <%= f.email_field :email, placeholder: t('view.email'),autofocus: true, class:'form-control', id: 'Email', autocomplete: "email" %>
<!--        </div>-->

<!--        <div class="form-control">-->
          <%= f.password_field :password,placeholder: t('view.user.password'), class:'form-control', id: 'Password', autocomplete: "current-password" %>
<!--        </div>-->

        <div class="btn btn-lg btn-block mt-2">
          <%= f.submit t('view.log_in'), class: "btn btn-primary"%>
        </div>
      <% end %>
      <%= render "devise/shared/links" %>

      <%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
        <div class="test_user">
          <%= f.hidden_field :email, value: 'test_user@examp.com' %>
          <%= f.hidden_field :password, value: 'password' %>
          <%= f.submit 'テストログイン', class: 'btn btn-secondary m-4' %>
        </div>
      <% end %>
    </div>
  </div>
</div>