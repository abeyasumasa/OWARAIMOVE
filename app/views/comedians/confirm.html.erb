<h3>以下の内容で、送信する。</h3>
<%= form_with(model: @comedian, local: true) do |form| %>

  <div class="comedian_icon">
    <%= image_tag(@comedian.combination_icon.url) if @comedian.combination_icon && @comedian.combination_icon.url %>
    <%= form.hidden_field :combination_icon_cache %>
  </div>

  <div class="comedian_comment">
    <p><%= t('view.comedian.comment') %>:<%= @comedian.comment %></p>
    <%= form.hidden_field :comment %>
  </div>

  <div class="comedian_combination_name">
    <p><%= t('view.comedian.combination_name') %>:<%= @comedian.combination_name %></p>
    <%= form.hidden_field :combination_name %>
  </div>

  <div class="comedian_email">
    <p><%= t('view.email') %>:<%= @comedian.email %></p>
    <%= form.hidden_field :email %>
  </div>

  <div class="comedian_genre">
    <p><%= t('view.comedian.genre') %><%= @comedian.genre %></p>
    <%= form.hidden_field :genre %>
  </div>

  <div class="comedian_twitter_url">
    <p>Twitter_url:<%= @comedian.twitter_url %></p>
    <%= form.hidden_field :twitter_url %>
  </div>

  <div class="comedian_youtube_url">
    <p>Youtube_url:<%= @comedian.youtube_url %></p>
    <%= form.hidden_field :youtube_url %>
  </div>

  <%= form.submit t('view.comedian.entry') %>
<% end %>

<%= form_with(model: @comedian, url: new_comedian_path, local: true, method: "get") do |form| %>
  <%= form.hidden_field :combination_icon_cache %>
  <%= form.submit t('view.back'), name: 'back' %>
<% end %>
